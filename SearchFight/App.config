<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <appSettings>
    
    <add key="GoogleUrl" value="https://www.googleapis.com/customsearch/v1?key={0}&amp;cx={1}&amp;q={2}"/>
    <add key="GoogleApi" value="YOUR-API-KEY"/>
    <add key="GoogleKey" value="YOUR-GOOGLE-CUSTOM-ENGINE-KEY"/>

    <add key="BingUrl" value="https://api.cognitive.microsoft.com/bing/v7.0/search"/>
    <add key="BingApi" value="YOUR-BING-API-KEY"/>
    
    <add key="BingSubscriptionTag" value="Ocp-Apim-Subscription-Key"/>
    
  </appSettings>
</configuration>
